package com_pcwk_ehr05;

import com_pcwk_ehr_cmn.pcwLog;

public class StringCastingMain implements pcwLog{

	public static void main(String[] args) {
		int age = 23;
		
		//int를 string으로 변환
		String strAge = age +"";
		System.out.printf("strAge=%s, int=%d\n",strAge,age);
		
		
		
		Integer.parseInt("23");//문자열 INT를 -> INT
		Integer.parseInt("23.25");//문자열 더블을 -> double
		
		Integer.valueOf("23");//int
		Double.valueOf("23+0.25");//double
		Float.valueOf("23.25");//float
		
		
		
	}

}
